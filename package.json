{
  "name": "@liquid-labs/catalyst-core-ui",
  "version": "0.0.10",
  "description": "Core Catalyst app libraries.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist/",
    "bin/",
    ".catalyst"
  ],
  "bin": {
    "react-local-websvr.sh": "bin/react-local-websvr.sh"
  },
  "scripts": {
    "build": "catalyst-scripts \"$PWD\" build",
    "start": "catalyst-scripts \"$PWD\" start",
    "pretest": "babel --source-maps src -d dist",
    "test": "jest --coverage dist",
    "prepare": "rm -rf dist && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liquid-Labs/catalyst-core.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Zane Rockenbaugh <zane@liquid-labs.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Liquid-Labs/catalyst-core/issues"
  },
  "homepage": "https://github.com/Liquid-Labs/catalyst-core#readme",
  "devDependencies": {
    "@liquid-labs/catalyst-scripts": "^1.0.0-alpha.6",
    "@liquid-labs/regex-repo": "^1.0.0-alpha.2",
    "jest": "^23.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.reduce": "^4.6.0",
    "moment-timezone": "^0.5.23"
  },
  "dependencies": {
    "@liquid-labs/redux-followup": "0.0.2",
    "@material-ui/icons": "^3.0.1",
    "mdi-material-ui": "^5.7.0",
    "normalizr": "^3.2.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "peerDependencies": {
    "@liquid-labs/mui-extensions": "0.0.7",
    "@liquid-labs/regex-repo": "1.0.0-alpha.2",
    "@material-ui/core": "^1.5.1 || 3.x",
    "classnames": "^2.2.6",
    "lodash.camelcase": "^4.3.0",
    "lodash.merge": "^4.6.1",
    "lodash.omit": "^4.5.0",
    "lodash.reduce": "^4.6.0",
    "lodash.upperfirst": "^4.3.1",
    "moment-timezone": "^0.5.21",
    "react": "16.x",
    "react-redux": "5.x",
    "react-router-dom": "4.x",
    "recompose": "^0.30.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "_catServices": [
    {
      "name": "react-dev-server",
      "interface-classes": [
        "http-html"
      ],
      "platform-types": [
        "local"
      ],
      "purposes": [
        "dev"
      ],
      "ctrl-scripts": [
        "react-local-websvr.sh"
      ],
      "params-req": [
        "CAT_SCRIPT_CORE_UI_WEB_APP_DIR"
      ],
      "params-opt": []
    }
  ]
}
