{
  "name": "@liquid-labs/catalyst-core-ui",
  "version": "1.0.0-prototype.0",
  "description": "Core Catalyst app libraries.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist/",
    "bin/",
    ".catalyst"
  ],
  "bin": {
    "react-dev-server.sh": "bin/react-dev-server.sh"
  },
  "scripts": {
    "prebuild": "rm -rf dist/* && mkdir -p dist",
    "build": "catalyst-scripts build",
    "start": "catalyst-scripts start",
    "pretest": "catalyst-scripts pretest",
    "test": "catalyst-scripts test",
    "prepare": "npm run build",
    "watch": "catalyst-scripts watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liquid-Labs/catalyst-core.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Zane Rockenbaugh <zane@liquid-labs.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Liquid-Labs/catalyst-core/issues"
  },
  "homepage": "https://github.com/Liquid-Labs/catalyst-core#readme",
  "devDependencies": {
    "@liquid-labs/catalyst-scripts": "latest",
    "@liquid-labs/regex-repo": "latest",
    "lodash.camelcase": "^4.3.0",
    "moment-timezone": "^0.5.21"
  },
  "peerDependencies": {
    "@liquid-labs/mui-extensions": "latest",
    "@liquid-labs/react-await": "latest",
    "@liquid-labs/regex-repo": "latest",
    "@material-ui/core": "^1.5.1 || 3.x",
    "@material-ui/icons": "^3.0.2",
    "classnames": "^2.2.6",
    "lodash.camelcase": "^4.3.0",
    "lodash.merge": "^4.6.1",
    "lodash.omit": "^4.5.0",
    "lodash.reduce": "^4.6.0",
    "lodash.upperfirst": "^4.3.1",
    "mdi-material-ui": "^5.9.0",
    "moment-timezone": "^0.5.21",
    "normalizr": "^3.2.4",
    "prop-types": "^15.7.2",
    "react": "16.x",
    "react-redux": "5.x || 6.x",
    "react-router-dom": "4.x",
    "recompose": "^0.30.0",
    "redux": "^4.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "_catServices": [
    {
      "name": "react-dev-server",
      "interface-classes": [
        "html"
      ],
      "platform-types": [
        "local"
      ],
      "purposes": [
        "dev"
      ],
      "ctrl-scripts": [
        "react-dev-server.sh"
      ],
      "params-req": [
        "CAT_SCRIPT_CORE_UI_WEB_APP_DIR"
      ],
      "params-opt": []
    }
  ]
}
